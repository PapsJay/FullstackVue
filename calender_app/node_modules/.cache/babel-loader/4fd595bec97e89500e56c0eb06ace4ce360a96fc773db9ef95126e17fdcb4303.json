{"ast":null,"code":"import { store } from '../store.js';\nexport default {\n  name: 'CalendarEvent',\n  props: ['event', 'day'],\n  computed: {\n    getEventBackgroundColor() {\n      const colors = ['#FF9999', '#85D6FF', '#99FF99'];\n      let randomColor = colors[Math.floor(Math.random() * colors.length)];\n      return `background-color: ${randomColor}`;\n    }\n  },\n  methods: {\n    editEvent(dayId, eventDetails) {\n      store.editEvent(dayId, eventDetails);\n    }\n  }\n};","map":{"version":3,"names":["store","name","props","computed","getEventBackgroundColor","colors","randomColor","Math","floor","random","length","methods","editEvent","dayId","eventDetails"],"sources":["/home/aruns/Desktop/VuePro/CookBook/hello-world/src/app-6/components/CalendarEvent.vue"],"sourcesContent":["<template>\n  <div class=\"day-event\" :style=\"getEventBackgroundColor\">\n    <div v-if=\"!event.edit\">\n      <span class=\"has-text-centered details\">{{ event.details }}</span>\n      <div class=\"has-text-centered icons\">\n        <i class=\"fa fa-pencil-square edit-icon\" \n          @click=\"editEvent(day.id, event.details)\"></i>\n        <i class=\"fa fa-trash-o delete-icon\"></i>\n      </div>\n    </div>\n    <div v-if=\"event.edit\">\n      <input type=\"text\" :placeholder=\"event.details\"/>\n      <div class=\"has-text-centered icons\">\n        <i class=\"fa fa-check\"></i>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { store } from '../store.js';\n\nexport default {\n  name: 'CalendarEvent',\n  props: ['event', 'day'],\n  computed: {\n    getEventBackgroundColor() {\n      const colors = ['#FF9999', '#85D6FF', '#99FF99'];\n      let randomColor = colors[Math.floor(Math.random() * colors.length)];\n      return `background-color: ${randomColor}`;\n    }\n  },\n  methods: {\n    editEvent (dayId, eventDetails) {\n      store.editEvent(dayId, eventDetails);\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.day-event {\n  margin-top: 6